#include <bits/stdc++.h>

using namespace std;

#define all(x) x.begin(), x.end()
#define sz(x) (int) x.size()
#define debug(x) cout << "DEBUG: " << x << endl;

typedef long long ll;
typedef unsigned long long ull;
typedef pair <int, int> ii;

int main(){
  cin.tie(NULL);
  ios_base::sync_with_stdio(false);
  int n;
  cin >> n;
  set <int> ans;
  for(int i = 0; i < n; i++){
    int x;
    cin >> x;
    ans.insert(x);
  }
  cout << sz(ans) << "\n";
  return 0;
}
